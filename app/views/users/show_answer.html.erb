

<table border=1>
<th>Answer ID</th>
<th>Question ID</th>
<th>Answer</th>
<th>Answered By</th>
<% if @answers %>
  <% @answers.each do |answer|%>
 <tr>
  <td><%= answer.id %></td>
    <td><%= answer.question_id %></td>
    <td><%= answer.answer %></td>
    <td><%= answer.answered_by %></td>
</tr>
<% end %>
<% else %>
 No Data
    <% end %>
    </table>





 