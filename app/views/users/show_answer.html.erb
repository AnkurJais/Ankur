<table border=1>
  <th>Answer ID</th>
  <th>Question ID</th>
  <th>Answered By</th>
  <th>Score</th>
  <% if @answers %>
    <% @answers.each do |answer|%>
      <tr>
        <td><%= answer.id %></td>
        <td><%= answer.question_id %></td>
        <td><%= answer.answer %></td>
        <td><%= answer.answered_by %></td>
        <td><%= link_to "Like" %> </td>
      </tr>
    <% end %>
  <% else %>
    No Data
  <% end %>
</table>





 